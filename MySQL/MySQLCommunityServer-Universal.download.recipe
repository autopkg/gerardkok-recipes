<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Description</key>
        <string>Download recipe for MySQL Community Server.  Downloads both the ARM and Intel builds.</string>
        <key>Identifier</key>
        <string>com.github.gerardkok.download.MySQLCommunityServer-Universal</string>
        <key>Input</key>
        <dict>
            <key>NAME</key>
            <string>MySQLCommunityServer</string>
            <key>RELEASE</key>
            <string>8.0</string>
            <key>INTEL_ARCHITECTURE</key>
            <string>x86_64</string>
            <key>ARM_ARCHITECTURE</key>
            <string>arm64</string>
            <key>USER_AGENT</key>
            <string>Mozilla/5.0 (Macintosh; Intel Mac OS X 10_8_3) AppleWebKit/536.28.10 (KHTML, like Gecko) Version/6.0.3 Safari/536.28.10</string>
        </dict>
        <key>MinimumVersion</key>
        <string>0.3.1</string>
        <key>Process</key>
        <array>
            <dict>
                <key>Processor</key>
                <string>URLTextSearcher</string>
                <key>Arguments</key>
                <dict>
                    <key>url</key>
                    <string>https://dev.mysql.com/downloads/mysql/%RELEASE%.html</string>
                    <key>re_pattern</key>
                    <string>\((?P&lt;rel_url&gt;mysql-(?P&lt;version&gt;[0-9\.]+)-.*-).*?dmg\)</string>
                    <key>request_headers</key>
                    <dict>
                        <key>user-agent</key>
                        <string>%USER_AGENT%</string>
                    </dict>
                </dict>
            </dict>
            <dict>
                <key>Processor</key>
                <string>URLDownloader</string>
                <key>Arguments</key>
                <dict>
                    <key>url</key>
                    <string>https://dev.mysql.com/get/Downloads/MySQL-%RELEASE%/%rel_url%%INTEL_ARCHITECTURE%.dmg</string>
                    <key>prefetch_filename</key>
                    <string>True</string>
                </dict>
            </dict>
            <dict>
                <key>Processor</key>
                <string>EndOfCheckPhase</string>
                <key>Arguments</key>
                <dict>
                    <key>intel_path</key>
                    <string>%pathname%</string>
                    <key>intel_version</key>
                    <string>%version%</string>
                </dict>
            </dict>
            <dict>
                <key>Processor</key>
                <string>URLDownloader</string>
                <key>Arguments</key>
                <dict>
                    <key>url</key>
                    <string>https://dev.mysql.com/get/Downloads/MySQL-%RELEASE%/%rel_url%%ARM_ARCHITECTURE%.dmg</string>
                    <key>prefetch_filename</key>
                    <string>True</string>
                </dict>
            </dict>
            <dict>
                <key>Processor</key>
                <string>EndOfCheckPhase</string>
                <key>Arguments</key>
                <dict>
                    <key>arm_path</key>
                    <string>%pathname%</string>
                    <key>arm_version</key>
                    <string>%version%</string>
                </dict>
            </dict>
        </array>
    </dict>
</plist>
